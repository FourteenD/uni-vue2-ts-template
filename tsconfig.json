{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@workspace/ts-config/uni-app.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "webpack-env",
      "@dcloudio/types",
      "miniprogram-api-typings",
      "mini-types"
    ],
    "paths": {
      "@/*": ["./src/*"],
      "~/*": ["./*"]
    }
  },
  "exclude": ["node_modules", "unpackage", "dist", "src/**/*.nvue"]
}
