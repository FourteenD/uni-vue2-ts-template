    "turbo": "^1.9.3",
    "typescript": "~4.5.5",
    "vue-template-compiler": ">= 2.6.14 < 2.7"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16.15.1",
    "pnpm": ">=8.1.0"
  },
  "uni-app": {
    "scripts": {}
  }
